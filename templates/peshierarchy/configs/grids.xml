			<modelfield>hierarchytype</modelfield>
		</col>
		<col role="field" filter="true" sort="true" foreignkey="true" foriegnkeylabel="name"> 
			<label>Company</label>
			<type>text</type>
			<parname>company</parname>
			<modelfield>company</modelfield>
			<class>gridfiltersize-medium</class>
		</col>
		<col role="field" filter="true" sort="true">
            <type>date</type>
            <parname>frm</parname>
            <label>Effectivity Date From</label>
            <modelfield>datefrom</modelfield>
            <class>gridfiltersize-small</class>
        </col>
        <col role="field" filter="true" sort="true">
            <type>date</type>
            <parname>to</parname>
            <label>Effectivity Date To</label>
            <modelfield>dateto</modelfield>
            <class>gridfiltersize-small</class>
        </col>
		<col role="controls" filter="true" modelproperty="true" filter-submit="true" mod_id="kg-cs-cs-ls">
	        <type>text</type>
	        <parname>icons</parname>
	        <class>gridfiltersize-small align-right</class>
