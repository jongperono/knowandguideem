<grid>
	<hierarchy_grid numcol="10">
		<col role="counter">
			<lable>#</lable>
		</col>
		<col role="field" filter="true" sort="true">
			<label>Code</label>
			<type>text</type>
			<parname>code</parname>
			<modelfield>numcode</modelfield>
		</col>
		<col role="field" filter="true" sort="true">
			<label>Description</label>
			<type>text</type>
			<parname>description</parname>
			<modelfield>description</modelfield>
		</col>
			<col role="field" filter="true" sort="true">
			<label>GP Code Div.</label>
			<type>text</type>
			<parname>gpcode_div</parname>
			<modelfield>gpcode_div</modelfield>
			<class>gridfiltersize-small</class>
		</col>
			<col role="field" filter="true" sort="true">
			<label>GP Code Dpt.</label>
			<type>text</type>
			<parname>gpcode_dept</parname>
			<modelfield>gpcode_dept</modelfield>
